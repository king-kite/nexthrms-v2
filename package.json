{
	"name": "hrms",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"dev:analyze": "cross-env ANALYZE=1 next dev",
		"build": "next build",
		"build:analyze": "cross-env ANALYZE=1 next build",
		"build:source-map": "source-map-explorer .next/static/**/*.js --no-border-checks",
		"start": "next start",
		"lint": "next lint",
		"createsuperuser": "cross-env SALT_ROUNDS=10 DATABASE_NAME=kitehrms ts-node ./src/db/seed/commands/superuser.ts",
		"merge-schema": "cat *.prisma > schema.prisma"
	},
	"prisma": {
		"schema": "./src/db/schema.prisma",
		"seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} ./src/db/seed/index.ts"
	},
	"engines": {
		"node": "^18.16.0",
		"npm": "^9.5.1"
	},
	"dependencies": {
		"@tanstack/react-query": "^4.2.1",
		"axios": "^0.27.2",
		"cookie": "^0.5.0",
		"formidable": "^2.0.1",
		"kite-react-tailwind": "^0.4.2",
		"next": "12.2.2",
		"next-connect": "^0.13.0",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-icons": "^4.4.0",
		"sharp": "^0.31.1",
		"yup": "^1.2.0"
	},
	"devDependencies": {
		"@next/bundle-analyzer": "^13.4.7",
		"@types/cookie": "^0.5.1",
		"@types/formidable": "^2.0.5",
		"@types/node": "18.0.6",
		"@types/react": "18.0.15",
		"@types/react-dom": "18.0.6",
		"autoprefixer": "^10.4.12",
		"cross-env": "^7.0.3",
		"eslint": "8.20.0",
		"eslint-config-next": "12.2.2",
		"postcss": "^8.4.18",
		"source-map-explorer": "^2.5.3",
		"tailwindcss": "^3.1.8",
		"ts-node": "^10.9.1",
		"typescript": "4.7.4"
	}
}
